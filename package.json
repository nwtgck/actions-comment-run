    "io-ts": "^2.2.10",
    "lodash": "^4.17.21",
    "marked": "^4.0.7",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/marked": "^4.0.1",
    "@types/node": "^16.18.25",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
